{
    "name": "wakeworks/privacyembed",
    "description": "An iframe embedding module that asks for consent before loading the content",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "iframe",
        "embed",
        "privacy",
        "gdpr"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.4",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "WakeWorks\\PrivacyEmbed\\": "src/",
            "WakeWorks\\PrivacyEmbed\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/js",
            "client/css",
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
