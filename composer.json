{
    "name": "wakeworks/privacyembed",
    "description": "An iframe embedding module that asks for consent before loading the content",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "wake up page design",
            "homepage": "https://wake.de"
        },
        {
            "name": "Jan Metzger",
            "homepage": "https://github.com/Zazama"
        }
    ],
    "keywords": [
        "silverstripe",
        "iframe",
        "embed",
        "privacy",
        "gdpr"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5",
        "silverstripe/admin": "^2"
    },
    "autoload": {
        "psr-4": {
            "WakeWorks\\PrivacyEmbed\\": "src/",
            "WakeWorks\\PrivacyEmbed\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/js",
            "client/css",
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
